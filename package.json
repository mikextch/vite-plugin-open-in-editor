{
  "name": "vite-plugin-open-in-editor",
  "version": "0.0.4",
  "description": "Vite plugin to open files directly in any editor (Cursor, VSCode, etc) from Alt+Click or Qwik dev tools.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cursor",
    "vite",
    "plugin",
    "open-in-editor",
    "qwik",
    "editor",
    "devTools"
  ],
  "author": "mikext",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikextch/vite-plugin-open-in-editor.git"
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^24.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
